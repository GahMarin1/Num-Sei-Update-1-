local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Num Sei (Update 1)", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- Services
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")

-- World IDs
local worldIDs = {
["CryptedAc"] = 6778163810
}

-- Function to teleport all players
local function teleportAllPlayers(worldID)
    for _, player in pairs(Players:GetPlayers()) do
        TeleportService:Teleport(worldID, player)
    end
end

-- Main Tab
local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- World Selection Dropdown
MainTab:AddDropdown({
    Name = "Select World",
    Default = "CryptedAc",
    Options = {"CryptedAc"},
    Callback = function(value)
        teleportAllPlayers(worldIDs[value])
    end
})

-- Initialize
OrionLib:Init()
